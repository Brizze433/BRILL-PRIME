modules = ["cpp-clang14", "c-clang14", "swift-5.8", "dart-2.18", "dart-3.5", "java-graalvm22.3", "nix", "swift-5.6", "web", "gcloud", "bash", "deno-2"]
run = "flutter run"

[nix]
channel = "stable-24_05"
packages = ["gnomeExtensions.window-calls-extended", "ipscan"]

[deployment]
run = ["sh", "-c", "flutter run"]

[workflows]
runButton = "Flutter App"

[[workflows.workflow]]
name = "Run Tests"
author = 40669021
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter test"

[[workflows.workflow]]
name = "Flutter App"
author = 40669021
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter pub get"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter run --web-port 5000 --web-hostname 0.0.0.0"
